<div>
	<input id="messageInput" type="text" name="message" placeholder="@bora SOS javascript">
	<label id="msgResponse"></label>

	<script src="/js/jquery-3.3.1.js"></script>
	<script type="text/javascript">
		$('#messageInput').keypress(function (e) {
			if(e.keyCode == 13){
				var msg = $('#messageInput').serialize();

				jQuery.ajax({
					type: "POST",
					url: "/event/create",
					data: msg,
					success: function()
					{
					}
				});
				document.getElementById('msgResponse').innerHTML = msg;
				return false;
			}
		});
	</script>
</div>